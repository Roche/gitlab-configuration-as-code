#!/usr/bin/env python3
from gcasc import GitlabConfigurationAsCode


def welcome():
    print(r"""
 _____  _____              _____ 
|  __ \/  __ \            /  __ \
| |  \/| /  \/  __ _  ___ | /  \/
| | __ | |     / _` |/ __|| |    
| |_\ \| \__/\| (_| |\__ \| \__/\
 \____/ \____/ \__,_||___/ \____/
    """, flush=True)


if __name__ == '__main__':
    welcome()
    GitlabConfigurationAsCode().configure()
